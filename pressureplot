import matplotlib.pyplot as plt
import datetime

filename = ('pressure_log', datetime.datetime.now().strftime("%Y%m%d"))
filename = str(filename)
data = open(filename, 'r')

timelist = []
pressurelist = []
templist = []


for line in data.readlines():
    line = line.split(' ')

    print(line)
    timelist.append(line[3])
    pressurelist.append(line[2])
    templist.append(line[1])

del timelist[0]
del pressurelist[0]
del templist[0]

plt.subplot(1,2,1)
plt.plot(timelist, pressurelist)
plt.subplot(1,2,2)
plt.plot(timelist, templist)
plt.gcf().autofmt_xdate()
plt.show()
