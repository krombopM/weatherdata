import matplotlib.pyplot as plt
import datetime

filename = ('pressure_log', datetime.datetime.now().strftime("%Y%m%d"))
filename = str(filename)
data = open(filename, 'r')

timelist = []
pressurelist = []

for line in data.readlines():
    line = line.split(' ')
    #del line[0]
    print(line)
    timelist.append(line[2])
    pressurelist.append(line[1])

del timelist[0]
del pressurelist[0]

plt.plot(timelist, pressurelist)
plt.gcf().autofmt_xdate()
plt.show()
